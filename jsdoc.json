{
  "source": {
    "include": [
      "README.md",
      "src/core/BaseLfo.js",

      "src/operator/Magnitude.js",
      "src/operator/MinMax.js",
      "src/operator/MovingAverage.js",
      "src/operator/MovingMedian.js",
      "src/operator/RMS.js",
      "src/operator/Slicer.js",

      "src/source/AudioInBuffer.js",
      "src/source/AudioInNode.js",
      "src/source/EventIn.js",

      "src/sink/BpfDisplay.js",
      "src/sink/Logger.js",
      "src/sink/SignalDisplay.js",
      "src/sink/WaveformDisplay.js",

      "src/utils/DisplaySync.js"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs",
    "projectTitle": "WavesJS - Low Frequency Operators",
    "template": "node_modules/jsdoc-template"
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    },
    "footer": "custom footer, insert ircam logo etc."
  }
